{
    "name": "h1ve-api",
    "version": "0.1.0",
    "description": "h1ve API",
    "engines": {
        "node": "11.9.0"
    },
    "main": "index.js",
    "scripts": {
        "lint": "eslint src/*.js --ignore-path .gitignore --fix",
        "build": "babel src -d dist",
        "dev": "npm run lint && nodemon --exec babel-node src/server.js",
        "start": "npm run build && node dist/server.js",
        "test": "mocha --require @babel/register"
    },
    "author": "Colin Lim",
    "license": "ISC",
    "dependencies": {
        "archetype-js": "^0.6.1",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "dotenv": "^6.2.0",
        "express": "^4.16.4",
        "express-jwt": "^5.3.1",
        "jsonwebtoken": "^8.4.0",
        "meta-auth": "^1.1.2",
        "mongoose": "^5.4.9"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/register": "^7.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.13.0",
        "mocha": "^5.2.0",
        "nodemon": "^1.18.9"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "parser": "babel-eslint",
        "rules": {
            "no-console": "off"
        },
        "env": {
            "node": true
        }
    }
}
